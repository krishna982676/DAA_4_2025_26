#include<bits/stdc++.h>
using namespace std;

long long operations = 0;
int maxDepth = 0;

void complexRec(int n, int depth) {
    maxDepth = max(maxDepth, depth);

    if (n <= 2) {
        operations++;
        return;
    }

    int p = n;
    while (p > 0) {
        vector<int> temp(n);
        for (int i = 0; i < n; i++) {
            temp[i] = i ^ p;
            operations++;
        }
        p >>= 1;
    }

    vector<int> small(n);
    for (int i = 0; i < n; i++) {
        small[i] = i * i;
        operations++;
    }

    reverse(small.begin(), small.end());
    operations++;

    complexRec(n / 2, depth + 1);
    complexRec(n / 2, depth + 1);
    complexRec(n / 2, depth + 1);
}

int main() {
    int t;
    cout << "Enter number of test cases: ";
    if (!(cin >> t) || t <= 0) {
        cerr << "Invalid number of test cases.\n";
        return 1;
    }

    vector<int> testCases(t);
    cout << "Enter values for each test case (n): ";
    for (int i = 0; i < t; ++i) {
        cin >> testCases[i];
    }

    for (int n_val : testCases) {
        operations = 0;
        maxDepth = 0;

        auto start = std::chrono::high_resolution_clock::now();
        complexRec(n_val, 1);
        auto end = std::chrono::high_resolution_clock::now();

        auto timeTaken = std::chrono::duration_cast<std::chrono::milliseconds>(end - start);

        cout << "n = " << n_val << endl;
        cout << "Total Operations = " << operations << endl;
        cout << "Recursion Depth = " << maxDepth << endl;
        cout << "Execution Time = " << timeTaken.count() << " ms" << endl;
    }

    return 0;
}
